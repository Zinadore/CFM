<UserControl x:Class="CFM.AssignmentModule.Navigation.AssignmentButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CFM.AssignmentModule.Navigation"
             xmlns:inf="clr-namespace:CFM.Infrastructure;assembly=CFM.Infrastructure"
             xmlns:views="clr-namespace:CFM.AssignmentModule.Views"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Button  Command="{x:Static inf:ApplicationCommands.NavigateCommand}"
                CommandParameter="{x:Type views:AssignmentsView}">Assignments</Button>
</UserControl>
