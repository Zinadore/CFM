<UserControl x:Class="CFM.UnitModule.Navigation.UnitButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CFM.UnitModule.Navigation"
             xmlns:views="clr-namespace:CFM.UnitModule.Views"
             xmlns:inf="clr-namespace:CFM.Infrastructure;assembly=CFM.Infrastructure"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Button Command="{x:Static inf:ApplicationCommands.NavigateCommand}"
                CommandParameter="{x:Type views:UnitsView}">Units</Button>
    </Grid>
</UserControl>
